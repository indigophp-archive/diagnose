{
    "name": "indigophp/diagnose",
    "description": "Run diagnostics in your environment",
    "license": "MIT",
    "keywords": ["diagnostic", "test", "report"],
    "homepage": "https://indigophp.com",
    "authors": [
        {
            "name": "Márk Sági-Kazár",
            "email": "mark.sagikazar@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0"
    },
    "require-dev": {
        "league/climate": "~2.4.0",
        "ext-bcmath" : "*",
        "sensiolabs/security-checker": "~2.0.0",
        "codeception/codeception": "~2.0.0"
    },
    "suggest": {
        "league/climate": "Required for CLI Report",
        "ext-bcmath": "Required by Cpu Performance Test",
        "sensiolabs/security-checker": "A security checker for your composer.lock"
    },
    "autoload": {
        "psr-4": {
            "Indigo\\Diagnose\\": "src/"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
